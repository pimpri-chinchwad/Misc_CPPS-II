marks=input("Enter marks: ")
try:
    marks=float(marks)
    marks=round(marks)
    if (marks>90):
        print("Grade A")
    elif(marks>80):
        print("Grade B")
    elif(marks>70):
        print("Grade C")
    elif (marks>50):
        print("Grade D")
    elif (marks>=35):
        print("Grade E")
    else:
        print("Fail")
except:
    print("Please enter integer")
